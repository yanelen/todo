<!DOCTYPE html>
<html>
<head>
  <title>Todo</title>
  <%= stylesheet_link_tag    'application', 'bootstrap.min', 'jumbotron-narrow', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
</head>
<body style="background-repeat: no-repeat; background-color: #c2c9cc;">
<div class="container">
  <div class="jumbotron" style="opacity: 0.8;">
    <%= yield %>
  </div>
<%= javascript_include_tag 'bootstrap.min', 'ie-emulation-modes-warning', 'ie10-viewport-bug-workaround'  => true %>
</body>
</html>
